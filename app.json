{
  "expo": {
    "name": "Shree Santoshi Masala",
    "slug": "masala-spice-shop",
    "version": "1.0.0",
    "scheme": "masalaspiceshop",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.masalaspiceshop.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC_C1PlSB1jSXxYsgoYTc9DChdrR9lSlDI"
        }
      },
      "package": "com.masalaspiceshop.app",
      "versionCode": 11,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#f69000"
        }
      ],
      "expo-router",
      "expo-secure-store",
      "expo-sqlite",
      "expo-image-picker",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to help place the map pin and share your location with customers during deliveries."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "imageWidth": 200,
          "backgroundColor": "#FFF8F0",
          "android": {
            "image": "./assets/splash-icon.png",
            "imageWidth": 200,
            "backgroundColor": "#FFF8F0"
          },
          "ios": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#FFF8F0",
            "resizeMode": "contain"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/72-Regular.ttf",
            "./assets/fonts/72-Bold.ttf",
            "./assets/fonts/72-SemiBold.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "961d69d8-b045-4d5e-be9d-5cbc5736cd83"
      }
    }
  }
}
